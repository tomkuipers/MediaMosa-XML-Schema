<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:opensearch="http://a9.com/-/spec/opensearch/1.1/">

    <!-- targetNamespace="http://mediamosa.org/schemas/3.0" -->
    <xs:annotation>
        <xs:documentation>
            Draft version of XML Schema for MediaMosa 3.x example output, Tom Kuipers 
            Make this available online at http://mediamosa.org/schemas/3.0/mediamosa.xsd
            - minOccurs = 0 means optional. Default is 1 - maxOccurs = number of occurences.
            "unbounded" means as many occurences as wished. Default is 1
        </xs:documentation>
    </xs:annotation>
    
    <!-- <xs:import namespace="http://a9.com/-/spec/opensearch/1.1/" schemaLocation="opensearch.xsd"/> -->
    <xs:import namespace="http://a9.com/-/spec/opensearch/1.1/" />
    
    <!-- root element -->
    <xs:element name="response">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="header" type="HeaderType"/>
                <xs:element name="items" type="ItemsType"/>
            </xs:sequence>
            <xs:attribute name="version" use="required" type="xs:decimal"/>
        </xs:complexType>
    </xs:element>

    <!-- simple elements for re-use-->
    <xs:element name="action" type="xs:anyURI"/>
    <xs:element name="app_id">
        <xs:simpleType>
            <xs:restriction base="xs:int">
                <xs:minInclusive value="1"/>
            </xs:restriction>
        </xs:simpleType>
    </xs:element>
    <xs:element name="app_quota_mb">
        <xs:simpleType>
            <xs:restriction base="xs:int">
                <xs:minInclusive value="0"/>
            </xs:restriction>
        </xs:simpleType>
    </xs:element>
    <xs:element name="app_diskspace_used_mb">
        <xs:simpleType>
            <xs:restriction base="xs:int">
                <xs:minInclusive value="0"/>
            </xs:restriction>
        </xs:simpleType>
    </xs:element>
    <xs:element name="asset_id" type="xs:token"/>
    <xs:element name="build" type="xs:int"/>
    <xs:element name="class" type="xs:string"/>
    <xs:element name="code" type="xs:int"/>
    <xs:element name="changed" type="xs:dateTime"/>
    <xs:element name="created" type="xs:dateTime"/>
    <xs:element name="content_type" type="xs:string"/><!-- string value? -->
    <xs:element name="download_url" type="xs:anyURI"/>
    <xs:element name="error_description" type="xs:string"/>
    <xs:element name="is_empty_asset" type="xs:boolean"/><!-- drop prefix 'is_' -->
    <!-- <xs:element name="empty_asset" type="xs:boolean"/> -->
    <xs:element name="is_external" type="xs:boolean"/><!-- drop prefix 'is_' -->
    <!-- <xs:element name="external" type="xs:boolean"/> -->
    <xs:element name="group_id"/><!-- I assumed this would be a string (token) not an int, right? -->
    <xs:element name="id" type="xs:int"/>
    <xs:element name="info" type="xs:string"/>
    <xs:element name="job_type">
        <xs:simpleType>
            <xs:restriction base="xs:string">
                <!-- what other jobtypes are there? -->
                <xs:enumeration value="ANALYSE"></xs:enumeration>
            </xs:restriction>
        </xs:simpleType>
    </xs:element>
    <xs:element name="mediafile_duration" type="xs:time"/>
    <xs:element name="mediafile_container_type" type="xs:string"/>
    <xs:element name="message" type="xs:string"/>
    <xs:element name="name" type="xs:string"/>
    <xs:element name="oai_prefix" type="xs:string"/><!-- string value? -->
    <xs:element name="oai_namespaceuri" type="xs:string"/><!-- string value or anyUri? -->
    <xs:element name="oai_schemalocation" type="xs:string"/><!-- string value or anyUri? -->
    <xs:element name="output" type="xs:string"/>
    <xs:element name="owner" type="xs:string"/>
    <xs:element name="owner_id" type="xs:string"/>
    <xs:element name="play_proxy_url" type="xs:anyURI"/>
    <xs:element name="play_restriction_start" type="xs:dateTime"/>
    <xs:element name="play_restriction_end" type="xs:dateTime"/>
    <xs:element name="played" type="xs:int"/><!-- restriction minInclusive 0, or just empty when never watched? -->
    <xs:element name="isprivate" type="xs:boolean"/><!-- is_private, isprivate or private (my vote) be consequent, drop prefix 'is' -->
    <!-- <xs:element name="private" type="xs:boolean"/> -->
    <xs:element name="priority" type="xs:int"/>
    <xs:element name="progress" type="xs:float"/>
    <xs:element name="progress_id" type="xs:int"/><!-- is there a maximun value for this, e.g. more than int maxinclusive 2147483647  -->
    <xs:element name="is_protected" type="xs:string"/><!-- drop prefix 'is_' , example response is string! value 'DOMAIN_REALM' (enumeration?) -->
    <!---
    <xs:element name="protected">
        <xs:simpleType>
            <xs:restriction base="xs:string"> -->
                <!-- what other types are there? -->
                <!-- <xs:enumeration value="DOMAIN_REALM"/>
            </xs:restriction>
        </xs:simpleType>
    </xs:element>
    -->
    <xs:element name="provider_id"/><!-- don't know what to expect here, type/xs:restriction not specified, can be xs:string or xs:int -->
    <xs:element name="quota_available_mb">
        <xs:simpleType>
            <xs:restriction base="xs:int">
                <xs:minInclusive value="0"/>
            </xs:restriction>
        </xs:simpleType>
    </xs:element>
    <xs:element name="reference_id" type="xs:token"/><!-- not sure what to expect here, assumed a string value -->
    <xs:element name="started" type="xs:dateTime"/>
    <xs:element name="started_unix" type="xs:long"/>
    <xs:element name="status">
        <xs:simpleType>
            <xs:restriction base="xs:string">
                <!-- what other statuses are there? -->
                <xs:enumeration value="FINISHED"/>
            </xs:restriction>
        </xs:simpleType>
    </xs:element>
    <xs:element name="has_streamable_mediafiles" type="xs:boolean"/><!-- drop prefix 'has_' -->
    <!-- <xs:element name="streamable_mediafiles" type="xs:boolean"/> -->
    <xs:element name="supplement_id" type="xs:token"/><!-- or token (xs:MNTOKEN) -->
    <xs:element name="supplement_base64" type="xs:base64Binary"/>
    <xs:element name="tag" type="xs:string"/><!-- not sure what to expect here, assumed a string value -->
    <xs:element name="ticket_id" type="xs:token"/>
    <xs:element name="is_unappropiate" type="xs:boolean"/><!-- typo deprecated -->
    <xs:element name="is_unappropriate" type="xs:boolean"/><!-- drop prefix 'is_' -->
    <!-- <xs:element name="unappropriate" type="xs:boolean"/> -->
    <xs:element name="uploadprogress_url" type="xs:anyURI"/>
    <xs:element name="version" type="xs:string"/>
    <xs:element name="videotimestamp" type="xs:dateTime"/>
    <xs:element name="videotimestampmodified" type="xs:dateTime"/>
    <xs:element name="view_asset_url" type="xs:anyURI"/>
    <xs:element name="viewed" type="xs:int"/><!-- restriction minInclusive 0, or just empty when never watched? -->

    <xs:complexType name="HeaderType">
        <xs:sequence>
            <xs:element name="request" type="RequestType"/>
            <xs:element name="debug" type="DebugType"/><!-- is this always present? -->
            <xs:element name="version" type="xs:string"/>
            <!-- made this string because it also includes buildnumber and is not a decimal like the version attribute -->
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="RequestType">
        <xs:sequence>
            <xs:element name="resultId">
                <xs:simpleType>
                    <xs:restriction base="xs:int"/>
                </xs:simpleType>
            </xs:element>
            <xs:element name="result">
                <xs:simpleType>
                    <xs:restriction base="xs:string"/>
                </xs:simpleType>
            </xs:element>
            <xs:element name="description">
                <!-- don't know what to expect here, xs:restriction not specified -->
            </xs:element>
            <xs:element name="method">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <!-- possible values should be listed here -->
                        <xs:enumeration value="GET"/>
                        <xs:enumeration value="POST"/>
                        <xs:enumeration value="PUT"/>
                        <xs:enumeration value="DELETE"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="uri">
                <xs:simpleType>
                    <xs:restriction base="xs:anyURI"/>
                </xs:simpleType>
            </xs:element>
            <xs:element name="time">
                <xs:simpleType>
                    <xs:restriction base="xs:float"/>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="DebugType">
        <xs:sequence>
            <xs:element ref="class"/>
            <xs:element name="matched" type="MatchedType"/>
            <xs:element name="errors">
                <!-- don't know what to expect here, xs:restriction not specified -->
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="MatchedType">
        <xs:sequence>
            <xs:element name="method">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <!-- possible values should be listed here -->
                        <xs:enumeration value="GET"/>
                        <xs:enumeration value="POST"/>
                        <xs:enumeration value="PUT"/>
                        <xs:enumeration value="DELETE"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="uri">
                <xs:simpleType>
                    <xs:restriction base="xs:anyURI"/>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="ItemsType">
        <xs:choice minOccurs="0" maxOccurs="unbounded">
            <!-- the example XML uses 'asset' but this can be 'mediafile', 'user' etc.  -->
            <xs:element name="asset" type="AssetType"/>
            
            <xs:element name="errorcode" type="ErrorcodeType"/>
            
            <xs:element name="version" type="VersionType"/>
        </xs:choice>
    </xs:complexType>

    <!-- /acl/app [GET] -->
    <xs:complexType name="AppType">
        <xs:annotation>
            <xs:documentation>
                /acl/app [GET]
                <![CDATA[
                <item id="1">
                    <app_id>3</app_id>
                    <name>NIBG</name>
                    <play_proxy_url>http://videotheek_5_.hiero.nl/play/{asset_id}/{mediafile_id}/{mediafile_filename}</play_proxy_url>
                    <view_asset_url>http://videotheek_5_.hiero.nl/viewasset/{asset_id}</view_asset_url>
                    <download_url>http://download_5_url/{asset_id}/{mediafile_id}/{mediafile_filename}</download_url>
                </item>
                ]]>
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="app_id"/>
            <xs:element ref="name"/>
            <xs:element ref="play_proxy_url"/>
            <xs:element ref="view_asset_url"/>
            <xs:element ref="download_url"/>
        </xs:sequence>
    
    </xs:complexType>
    
    <!-- /app/quota [GET] -->
    <xs:complexType name="AppQuotaType">
        <xs:annotation>
            <xs:documentation>
                /app/quota [GET]
                <![CDATA[
                <item id="1">
                    <app_quota_mb>153600</app_quota_mb>
                    <app_diskspace_used_mb>2094</app_diskspace_used_mb>
                    <quota_available_mb>151506</quota_available_mb>
                </item>
                ]]>
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="app_quota_mb"/>
            <xs:element ref="app_diskspace_used_mb"/>
            <xs:element ref="quota_available_mb"/>
        </xs:sequence>
        
    </xs:complexType>
    
    <!-- /asset [GET] -->
    <xs:complexType name="AssetType">
        <xs:sequence>
            <!-- didn't define 'asset_id' since we use an attribute for that -->
            <xs:element ref="app_id"/>
            <xs:element ref="provider_id"/>
            <xs:element ref="owner_id"/>
            <xs:element ref="group_id"/>
            <xs:element ref="videotimestamp"/>
            <xs:element ref="videotimestampmodified"/>
            <xs:element ref="play_restriction_start"/>
            <xs:element ref="play_restriction_end"/>
            <xs:element ref="reference_id"/>
            <!-- for element 'isprivate' the type is defined in this schema,
                the 'type' attribute in the XML document is not needed.
                Other boolean elements, like 'is_favorite' or 'is_external' have an underscore. 
                To be consistent we should use it here also, or drop the 'is_' prefix completely. I would prefer that. 
                It is an indictator for storing a boolean value, but that is already denifed in the schema -->
            <xs:element name="private">
                <xs:simpleType>
                    <xs:restriction base="xs:boolean"/>
                </xs:simpleType>
            </xs:element>
            <xs:element ref="mediafile_duration"/>
            <xs:element ref="mediafile_container_type"/>
            <!-- see comments for element 'private', dropped 'is_' prefix -->
            <xs:element name="unappropriate">
                <xs:simpleType>
                    <xs:restriction base="xs:boolean"/>
                </xs:simpleType>
            </xs:element>
            <!-- I assume element 'is_unappropiate' is in the example XML by mistake -->
            <!-- see comments for element 'private', dropped 'is_' prefix -->
            <xs:element name="external">
                <xs:simpleType>
                    <xs:restriction base="xs:boolean"/>
                </xs:simpleType>
            </xs:element>
            <!-- see comments for element 'private', dropped 'is_' prefix -->
            <xs:element name="empty_asset">
                <xs:simpleType>
                    <xs:restriction base="xs:boolean"/>
                </xs:simpleType>
            </xs:element>
            <xs:element ref="viewed"/>
            <xs:element ref="played"/>
            <!-- see comments for element 'private', dropped 'is_' prefix -->
            <xs:element name="protected">
                <xs:simpleType>
                    <xs:restriction base="xs:boolean"/>
                </xs:simpleType>
            </xs:element>
            <xs:element name="asset_property_is_hidden">
                <xs:simpleType>
                    <xs:restriction base="xs:boolean"/>
                </xs:simpleType>
            </xs:element>
            <!-- see comments for element 'private', dropped 'is_' prefix -->
            <xs:element name="favorite">
                <xs:simpleType>
                    <xs:restriction base="xs:boolean"/>
                </xs:simpleType>
            </xs:element>
            <xs:element name="granted">
                <xs:simpleType>
                    <xs:restriction base="xs:boolean"/>
                </xs:simpleType>
            </xs:element>
            <xs:element name="url_still_oai">
                <xs:simpleType>
                    <xs:restriction base="xs:anyURI"/>
                </xs:simpleType>
            </xs:element>
            <xs:element name="url_still">
                <xs:simpleType>
                    <xs:restriction base="xs:anyURI"/>
                </xs:simpleType>
            </xs:element>
            <!-- see comments for element 'private', dropped 'has_' prefix -->
            <xs:element name="streamable_mediafiles">
                <xs:simpleType>
                    <xs:restriction base="xs:boolean"/>
                </xs:simpleType>
            </xs:element>
            <xs:element name="metadata">
                <xs:complexType>
                    <xs:sequence>
                        <!-- reference external xsd for czp:, dc:, qdc: namespaces -->
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="mediafiles">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="mediafile" type="MediafileType" minOccurs="0"
                            maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
        <!-- when attribute 'id' is used, element 'asset_id' is superfluous -->
        <xs:attribute name="id">
            <xs:simpleType>
                <xs:restriction base="xs:string"/>
            </xs:simpleType>
        </xs:attribute>
    </xs:complexType>

